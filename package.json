	"author": {
		"name": "rowrawer",
		"email": "rowrawer@gmail.com",
		"url": "https://github.com/rowrawer"
	},
	"repository": "github:rowrawer/setlist-generator",
	"version": "2.1.0",
	"private": true,
	"dependencies": {
		"@material-ui/core": "^4.11.0",
		"@material-ui/icons": "^4.9.1",
		"@material-ui/styles": "^4.10.0",
		"@nivo/pie": "^0.67.0",
		"array-move": "^3.0.1",
		"chance": "^1.1.7",
		"clipboard-copy": "^4.0.1",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"helmet": "^4.1.1",
		"install": "^0.13.0",
		"moment": "^2.29.0",
		"moment-duration-format": "^2.3.2",
		"npm": "^6.14.8",
		"react": "^16.13.1",
		"react-checkbox-tree": "^1.6.0",
		"react-dom": "^16.13.1",
		"react-scripts": "^3.4.3",
		"react-sortable-hoc": "^1.11.0",
		"react-spinners-kit": "^1.9.1",
		"spotify-web-api-node": "^5.0.2",
		"styled-components": "^5.2.0"
	},
	"scripts": {
		"start": "concurrently \"npm run server\" \"npm run client\"",
		"client": "set PORT=5415 && react-scripts start",
		"server": "nodemon server/server.js",
		"build": "react-scripts build",
		"prod": "concurrently \"serve -l 5415 -s build\" \"set NODE_ENV=production && node server.js\"",
		"test": "react-scripts test",
		"eject": "react-scripts eject"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"devDependencies": {
		"concurrently": "^5.3.0",
		"eslint-config-prettier": "^7.2.0",
		"eslint-plugin-prettier": "^3.1.4",
		"http-proxy-middleware": "^1.0.5",
		"prettier": "^2.1.2",
		"typescript": "^4.0.3"
	},
	"nodemonConfig": {
		"ignore": [
			"src/*",
			"package.json"
		]
	}
}
